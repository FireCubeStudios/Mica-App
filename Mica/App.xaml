                                                        </DoubleAnimation>
                                                        <DoubleAnimation Storyboard.TargetName="Glow"  Storyboard.TargetProperty="OffsetY" EnableDependentAnimation="True" Duration="0:0:0.0" To="0">
                                                        </DoubleAnimation>
